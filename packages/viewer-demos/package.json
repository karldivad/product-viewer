{
	"name": "@lowesinnovationlab/viewer-demos",
	"version": "0.0.6",
	"private": true,
	"engines": {
		"node": ">=10.0.0"
	},
	"scripts": {
		"preinstall": "npx npm-force-resolutions",
		"start": "react-scripts --max_old_space_size=4096 start",
		"build:watch": "npm run start",
		"build": "react-scripts --max_old_space_size=4096 build",
		"test": "react-scripts test --watchAll=false --passWithNoTests",
		"test:watch": "react-scripts test",
		"eject": "react-scripts eject",
		"fix": "npm run fix:eslint && npm run fix:prettier",
		"fix:eslint": "eslint --fix --ext .ts .",
		"fix:prettier": "prettier --write .",
		"precommit": "lint-staged",
		"lint": "npm run lint:eslint && npm run lint:prettier",
		"lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:prettier": "prettier --check .",
		"prepare": "symlink-dir ./node_modules/@lowesinnovationlab/viewer-assets/assets ./public/common-assets"
	},
	"dependencies": {
		"@lowesinnovationlab/product-viewer": "^0.0.6",
		"@material-ui/core": "^4.12.3",
		"@material-ui/icons": "^4.11.2",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.12",
		"@types/prismjs": "^1.16.6",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@types/react-redux": "^7.1.20",
		"@types/react-router-dom": "^5.3.2",
		"@types/styled-components": "^5.1.17",
		"prismjs": "^1.25.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.6",
		"react-router-dom": "^6.1.0",
		"react-scripts": "^5.0.0",
		"react-split-pane": "^0.1.92",
		"redux": "^4.1.2",
		"redux-devtools-extension": "^2.13.9",
		"redux-thunk": "^2.4.1",
		"sass": "^1.45.0",
		"styled-components": "^5.3.3",
		"typescript": "^4.0.2"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"lint-staged": {
		"*.{js,ts,jsx,tsx}": [
			"eslint",
			"prettier --check"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@lowesinnovationlab/viewer-assets": "^0.0.5",
		"@typescript-eslint/eslint-plugin": "^5.6.0",
		"@typescript-eslint/parser": "^5.6.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"fast-sass-loader": "^2.0.1",
		"is-svg": "^4.3.2",
		"lint-staged": "^12.1.2",
		"prettier": "^2.5.1",
		"symlink-dir": "^5.0.1"
	},
	"resolutions": {
		"ansi-html": "https://registry.yarnpkg.com/ansi-html-community/-/ansi-html-community-0.0.8.tgz",
		"ansi-regex": "5.0.1",
		"browserslist": "4.18.1",
		"cache-base": "^4.0.0",
		"ejs": "^3.1.6",
		"glob-parent": "6.0.2",
		"immer": "9.0.6",
		"mixin-deep": "^2.0.1",
		"node-forge": "^1.2.0",
		"nth-check": "2.0.1",
		"shell-quote": "^1.7.3"
	}
}
