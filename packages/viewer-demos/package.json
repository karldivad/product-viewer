{
	"name": "@lowesinnovationlab/viewer-demos",
	"version": "0.0.2",
	"private": true,
	"engines": {
		"node": ">=10.0.0"
	},
	"scripts": {
		"start": "react-scripts --max_old_space_size=4096 start",
		"build": "react-scripts --max_old_space_size=4096 build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"fix": "npm run fix:eslint && npm run fix:prettier",
		"fix:eslint": "eslint --fix --ext .ts .",
		"fix:prettier": "prettier --write .",
		"precommit": "lint-staged",
		"lint": "npm run lint:eslint",
		"lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:prettier": "prettier --check .",
		"prepare": "symlink-dir ./node_modules/@lowesinnovationlab/viewer-assets/assets ./public/common-assets"
	},
	"dependencies": {
		"@lowesinnovationlab/product-viewer": "^0.0.2",
		"@material-ui/core": "^4.11.4",
		"@material-ui/icons": "^4.11.2",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"@types/jest": "^24.9.1",
		"@types/node": "^12.20.15",
		"@types/prismjs": "^1.16.6",
		"@types/react": "^16.14.8",
		"@types/react-dom": "^16.9.13",
		"@types/react-redux": "^7.1.16",
		"@types/react-router-dom": "^5.1.7",
		"@types/styled-components": "^5.1.10",
		"prismjs": "^1.24.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-redux": "^7.2.4",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.3",
		"react-split-pane": "^0.1.92",
		"redux": "^4.1.0",
		"redux-devtools-extension": "^2.13.9",
		"redux-thunk": "^2.3.0",
		"sass": "^1.34.1",
		"styled-components": "^5.3.0",
		"typescript": "^3.9.9"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@lowesinnovationlab/viewer-assets": "^0.0.2",
		"@typescript-eslint/eslint-plugin": "^4.26.1",
		"@typescript-eslint/parser": "^4.26.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^6.13.0",
		"eslint-plugin-prettier": "^3.4.0",
		"fast-sass-loader": "^1.5.0",
		"is-svg": "^4.3.1",
		"lint-staged": "^10.5.4",
		"prettier": "^2.3.1",
		"symlink-dir": "^5.0.1"
	}
}
